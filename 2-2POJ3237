#include <iostream>
#include <cstring>
using namespace std;
/*3237:鸡兔同笼
总时间限制: 1000ms 内存限制: 65536kB
描述:
一个笼子里面关了鸡和兔子（鸡有2只脚，兔子有4只脚，没有例外）。
已经知道了笼子里面脚的总数a，问笼子里面至少有多少只动物，至多有多少只动物
输入:
第1行是测试数据的组数n，后面跟着n行输入。每组测试数据占1行，每行一个正整数a (a < 32768)
输出:
输出包含n行，每行对应一个输入,包含两个正整数，第一个是最少的动物数，第二个是最多的动物数，两个正整数用一个空格分开
如果没有满足要求的答案，则输出两个0。*/

int findMin(int a) {
	if (a % 4 == 0) {
		return a / 4;
	}
	else if (a % 4 == 2) {
		return a / 4 + 1;
	}
	else
		return 0;
}
int findMax(int a) {
	return a % 2 == 0 ? a / 2 : 0;
	/*if (a % 2 == 0) {
		return a / 2;
	}
	else
		return 0;*/
}

int main() {
	//输入
	int n;
	cin >> n;
	//if (n == EOF)
	//	return 0;
	//int *data = new int(n);
	int* data = new int[n];
	memset(data, 0, sizeof(int) * n);
	for (int i = 0; i < n; i++) {
		cin >> data[i];
	}
	for (int i = 0; i < n; i++) {
		cout<<findMin(data[i]);
		cout << " ";
		cout << findMax(data[i]);
		cout << endl;
	}
	return 0;

}
//编译错误1:
//'memset' was not declared in this scope
//memset的头文件为cstring，将其添加进程序中

//编码错误2：
//memset函数的使用样例：
//char buffer[20];
//memset(buffer, 0, sizeof(char) * 20);注意别忘记乘上数组中元素个数

//编码错误3：
//动态声明和初始化数组时，要注意int *data=new int[n];中的括号为方括号
//千万不要写成圆括号

//提交代码后显示RunTime Error，运行时错误
//说明出现数组越界或不合理使用指针
